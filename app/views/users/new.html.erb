<h1>New User</h1>

<%= form_for :user, url: users_path do |f| %>
  <p>
    <%= f.label :"E-mail" %><br>
    <%= f.text_field :Email %>
  </p>
 
  <p>
    <%= f.label :Password %><br>
    <%= f.text_field :Password %>
  </p>
 
  <p>
    <%= f.label :"Confirm Password" %><br>
    <%= f.text_field :ConfirmPassword %>
  </p>
  
  <p>
    <%= f.submit %>
  </p>
  
<% end %>